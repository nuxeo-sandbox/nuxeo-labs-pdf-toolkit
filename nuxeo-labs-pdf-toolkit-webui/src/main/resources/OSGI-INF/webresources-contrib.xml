<?xml version="1.0"?>

<component name="nuxeo.labs.pdf.toolkit.webui.resources.contrib">
  <!-- <require>org.nuxeo.web.ui.resources</require> -->
  <require>org.nuxeo.web.ui.properties.contrib</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="nuxeo-pdf-toolkit-bundle.html" type="import">
      <uri>/ui/nuxeo-pdf-toolkit/nuxeo-pdf-toolkit-bundle.html</uri>
    </resource>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="web-ui">
      <resources append="true">
        <resource>nuxeo-pdf-toolkit-bundle.html</resource>
      </resources>
    </bundle>
  </extension>

